{
  "name": "vindb-web-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc --build",
    "dev:old": "concurrently 'npm:watch-*'",
    "dev": "nodemon --watch src --watch ../shared/src --verbose --ext js,ts,json --exec \"tsc --build --verbose --pretty && node dist/app.js\"",
    "watch-ts": "tsc --build --verbose --watch --pretty",
    "watch-node": "nodemon --watch dist --verbose dist/app.js"
  },
  "dependencies": {
    "async": "^2.6.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.3.5",
    "dotenv": "^2.0.0",
    "express": "^4.16.3",
    "moment": "^2.27.0",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "serve-favicon": "^2.5.0",
    "sitemap": "^6.2.0",
    "squel": "~5.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/mysql": "^2.15.14",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7",
    "nodemon": "^2.0.4",
    "concurrently": "^5.2.0",
    "module-alias": "^2.2.2"
  },
  "_moduleAliases": {
    "@shared": "../shared/dist"
  }
}
